     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2 3 1 6 7\n",
      "1 2 3 6 7\n"
     ]
    }
   ],
   "source": [
    "n=map(int,input().split(\" \"))\n",
    "l=[]\n",
    "for i in n:\n",
    "    l.append(i)\n",
    "for j in range(1,len(l)):\n",
    "    temp=l[j]\n",
    "    k=j-1\n",
    "    while(k>=0 and l[k]>temp):\n",
    "        l[k+1]=l[k]\n",
    "        k-=1\n",
    "    l[k+1]=temp\n",
    "sep =\" \"\n",
    "print(sep.join(map(str,l)))"
   ]
  },
